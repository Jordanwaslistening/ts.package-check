{
  "name": "@skypack/package-score",
  "version": "0.0.1",
  "description": "A quality score checker for npm packages",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "exports": {
    "import": "./index.esm.mjs",
    "require": "./lib/index.js"
  },
  "bin": {
    "package-score": "./index.bin.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tsc --noEmit --noUnusedLocals true --noUnusedParameters true"
  },
  "keywords": [
    "skypack",
    "package",
    "quality",
    "lint"
  ],
  "files": [],
  "author": "Fred K. Schott",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "kleur": "^4.1.3"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skypackjs/package-score.git"
  },
  "bugs": {
    "url": "https://github.com/skypackjs/package-score/issues"
  },
  "homepage": "https://github.com/skypackjs/package-score#readme"
}
